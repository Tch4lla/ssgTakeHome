{
  "extends": "@tablecheck/scripts/tsconfig/base.json",
  "exclude": ["node_modules"],
  "include": [
    "cms/**/*.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/*.ts",
    "src/*.tsx",
    "src/**/*.json",
    "src/*.json"
  ],
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "target": "es5",
    "baseUrl": "src",
    "noEmit": true,
    "paths": { "#cypress/*": ["../cypress/*"] }
  }
}
